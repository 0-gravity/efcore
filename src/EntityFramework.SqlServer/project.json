{
    "version": "7.0.0-beta8",
    "description": "Microsoft SQL Server data store for Entity Framework.",
    "repository": {
        "type": "git",
        "url": "git://github.com/aspnet/entityframework"
    },
    "compilationOptions": {
        "warningsAsErrors": true
    },
    "dependencies": {
        "EntityFramework.Relational": "7.0.0-beta8"
    },
    "compile": "..\\Shared\\*.cs",
    "namedResource": {
        "EntityFramework.SqlServer.SqlServerStrings": "Properties/SqlServerStrings.resx"
    },
    "resource": [
        "Properties/EntityFramework.SqlServer.rd.xml"
    ],
    "frameworks": {
        "net45": {},
        "dnxcore50": {
            "dependencies": {
                "System.Data.SqlClient": "4.0.0-beta-23409",
                "System.Text.Encoding.CodePages": "4.0.1-beta-23409",
                "System.Threading.Thread": "4.0.0-beta-23409",
                "System.Threading.ThreadPool": "4.0.10-beta-23409"
            }
        },
        "dotnet": {
            "dependencies": {
                "System.Data.SqlClient": "4.0.0-beta-23409",
                "System.Threading.Thread": "4.0.0-beta-23409"
            }
        }
    }
}