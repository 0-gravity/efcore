{
    "version": "7.0.0-beta6",
    "description": "Shared Design-time Entity Framework components for relational data stores.",
    "repository": {
        "type": "git",
        "url": "git://github.com/aspnet/entityframework"
    },
    "compilationOptions": {
        "warningsAsErrors": true
    },
    "dependencies": {
        "Microsoft.AspNet.Razor": "4.0.0-beta6",
        "Microsoft.CodeAnalysis.CSharp": "1.0.0-*",
        "EntityFramework.Relational": "7.0.0-beta6"
    },
    "compile": "..\\Shared\\*.cs",
    "namedResource": {
        "EntityFramework.Relational.Design.Strings": "Properties/Strings.resx"
    },
    "frameworks": {
        "net45": {
            "frameworkAssemblies": {
                "System.IO": {
                    "version": "4.0.0.0",
                    "type": "build"
                },
                "System.Text.Encoding": {
                    "version": "4.0.0.0",
                    "type": "build"
                },
                "System.Threading.Tasks": {
                    "version": "4.0.0.0",
                    "type": "build"
                }
            }
        },
        "dnx451": {
            "frameworkAssemblies": {
                "System.IO": "4.0.0.0",
                "System.Text.Encoding": "4.0.0.0",
                "System.Threading.Tasks": "4.0.0.0"
            },
            "dependencies": {
                "Microsoft.Framework.Runtime.Roslyn": "1.0.0-beta6"
            }
        },
        "dnxcore50": {
            "dependencies": {
                "Microsoft.Framework.Runtime.Roslyn": "1.0.0-beta6"
            }
        }
    }
}