{
  "version": "1.0.0-rc2-final",
  "description": "Shared Entity Framework components for relational data stores.",
  "buildOptions": {
    "warningsAsErrors": true,
    "keyFile": "../../tools/Key.snk",
    "nowarn": [
      "CS1591"
    ],
    "xmlDoc": true,
    "compile": {
      "include": "../Shared/*.cs"
    },
    "embed": {
      "includeFiles": "Properties/Microsoft.EntityFrameworkCore.Relational.rd.xml"
    }
  },
  "dependencies": {
    "Microsoft.EntityFrameworkCore": "1.0.0-rc2-final"
  },
  "frameworks": {
    "net451": {
      "dependencies": {
        "System.Diagnostics.DiagnosticSource": "4.0.0-rc2-24027"
      },
      "frameworkAssemblies": {
        "System.Data": "",
        "System.Diagnostics.Tracing": {
          "type": "build"
        },
        "System.Runtime": {
          "type": "build"
        },
        "System.Threading": {
          "type": "build"
        },
        "System.Transactions": ""
      }
    },
    "netstandard1.3": {
      "imports": [
        "portable-net452+win81"
      ],
      "dependencies": {
        "Microsoft.CSharp": "4.0.1-rc2-24027",
        "System.Data.Common": "4.0.1-rc2-24027",
        "System.Diagnostics.DiagnosticSource": "4.0.0-rc2-24027",
        "System.Text.RegularExpressions": "4.0.12-rc2-24027"
      }
    },
    "netcore50": {
      "dependencies": {
        "Microsoft.NETCore.Platforms": {
          "type": "build",
          "version": "1.0.1-rc2-24027"
        },
        "Microsoft.CSharp": "4.0.1-rc2-24027",
        "System.Data.Common": "4.0.1-rc2-24027",
        "System.Text.RegularExpressions": "4.0.12-rc2-24027",
        "System.Diagnostics.DiagnosticSource": "4.0.0-rc2-24027",
        "System.Dynamic.Runtime": {
          "type": "build",
          "version": "4.0.11-rc2-24027"
        },
        "System.Runtime.Extensions": {
          "type": "build",
          "version": "4.1.0-rc2-24027"
        }
      }
    }
  }
}